{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "name": "blog",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "avatar": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    },
    "author": {
      "type": "string",
      "private": false,
      "required": true
    },
    "date": {
      "type": "date"
    },
    "readTime": {
      "type": "string",
      "required": false
    },
    "img": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    },
    "desc": {
      "type": "text",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "content": {
      "type": "richtext",
      "required": true
    },
    "category": {
      "via": "blogs",
      "model": "categories"
    }
  }
}
